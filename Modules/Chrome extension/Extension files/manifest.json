{
  "manifest_version": 2,

  "name": "RezMela",
  "description": "RezMela integration",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "RezMela"
  },
  "permissions": [
    "activeTab",
	"storage",
    "https://ajax.googleapis.com/",
	"http://www.rezmela.net/"
  ]
}